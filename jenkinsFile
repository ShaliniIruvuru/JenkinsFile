pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'mvn claen'
            }
        }

        stage('Test')  {
            steps {
                echo 'Testing..'
                sh 'mvn test'
            }
        }
                stage('Compile')  {
                    steps {
                        echo 'Compiling..'
                        sh 'mvn compile'
                    }
                }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}